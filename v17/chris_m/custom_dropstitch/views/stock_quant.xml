<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Inherit Form View -->
    <record id="stock_quant_view_form" model="ir.ui.view">
        <field name="name">stock.quant.view.form.inherit.module</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.view_stock_quant_tree_editable"/>
        <field name="arch" type="xml">
            <field name="reserved_quantity" position="after">
                <field name="free_qty"/>
                <field name="incoming_qty" optional="show"/>
                <field name="outgoing_qty" optional="show"/>
                <field name="virtual_available" optional="show"/>
            </field>
        </field>
    </record>
</odoo>